{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "api/**/*.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "api/overwrites/**",
          "samples/**/*.*",
          "**/includes/**",
          "***/contributing.md"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "_csharplang/spec",
        "dest": "csharp/language-reference/language-specification",
        "exclude": [
          "README.md",
          "LICENSE.md"
        ]
      },
      {
        "files": [
          "csharp-7.0/*.md",
          "csharp-7.1/*.md",
          "csharp-7.2/*.md",
          "csharp-7.3/*.md",
          "csharp-8.0/*.md"
        ],
        "src": "_csharplang/proposals",
        "dest": "csharp/language-reference/proposals",
        "exclude": [
          "**/README.md",
          "inactive/**",
          "rejected/**"        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "_vblang/spec",
        "dest": "visual-basic/reference/language-specification",
        "exclude": [
          "README.md",
          "LICENSE.md",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "core/**/*.md",
          "core/**/*.yml"
        ]
      },
      {
        "files": [
          "index.md"
        ]
      },
      {
        "files": [
          "toc.yml"
        ],
        "dest": "breadcrumb"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "samples/**/*.*",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/dotnet/breadcrumb/toc.json",
      "_displayLangs": ["csharp"],
      "dev_langs": ["csharp"],
      "ms.devlang": "dotnet",
      "author": "dotnet-bot",
      "ms.author": "dotnetcontent",
      "searchScope": [".NET"],
      "uhfHeaderId": "MSDocsHeader-DotNet",
      "apiPlatform": "dotnet",
      "ms.prod":"dotnet",
      "ms.topic": "conceptual",
